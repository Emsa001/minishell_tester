cat ./data/empty.txt > temp/empty_copy.txt
head -n 10 ./data/long.txt > temp/short.txt
tail -n 20 ./data/lorem.txt > temp/lorem_tail.txt
grep "pattern" ./data/lorem.txt > temp/lorem_pattern.txt
sed 's/search/replace/g' ./data/lorem.txt > temp/lorem_replaced.txt
sort -r ./data/long.txt > temp/long_sorted.txt
awk '{print $1}' ./data/long.txt > temp/first_column.txt
cut -d ',' -f 2 ./data/long.txt > temp/second_column.txt
grep -v "^$" ./data/newlines.txt > temp/no_empty_lines.txt
uniq ./data/lorem.txt > temp/lorem_unique.txt
